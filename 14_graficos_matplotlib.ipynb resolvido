        "plt.figure(figsize=(6,3))\n",
        "plt.plot(x,y)\n",
        "plt.title(\"Fun√ß√£o linear y=2x+1\")\n",
        "plt.xlabel(\"Eixo x\")\n",
        "plt.ylabel(\"Eixo y\")\n",
        "plt.show()"
