        "\n",
        "print(\"Conjunto de Dados:\")\n",
        "print(df_vendas)\n",
        "\n",
        "print(\"1.Gr√°fico de disper√ß√£o Pre√ßo(R$) vs Vendas(Unidades)\")\n",
        "plt.figure(figsize=(5,3))\n",
        "plt.scatter(df_vendas[\"Pre√ßo (R$)\"],df_vendas[\"Vendas (Unidades)\"])\n",
        "plt.title(\"Pre√ßo x Vendas\")\n",
        "plt.xlabel(\"Pre√ßo (R$)\")\n",
        "plt.ylabel(\"Vendas (Unidades)\")\n",
        "plt.show()"
